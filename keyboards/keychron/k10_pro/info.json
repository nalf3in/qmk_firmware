{
    "keyboard_name": "Keychron K10 Pro",
    "manufacturer": "Keychron",
    "url": "https://github.com/Keychron",
    "maintainer": "nal3in",
    "processor": "STM32L432",
    "bootloader": "stm32-dfu",
    "usb": {
    	"vid": "0x3434"
    },
    "features": {
        "bootmagic": true,
        "command": false,
        "console": false,
        "dip_switch": true,
        "extrakey": true,
        "mousekey": true,
        "nkro": true
    },
    "dip_switch": {
        "matrix_grid": [ [5, 4] ]
    },
    "matrix_size": {
        "rows": 6,
        "cols": 21
    },
    "matrix_pins": {
        "cols": ["B0", null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null],
        "rows": ["B5", "B4", "B3", "A15", "A14", "A13"],
        "custom": true,
        "custom_lite": true
    },
    "diode_direction": "ROW2COL"
}